<launch>

	<!-- launch map lidar provider -->
	<include file="$(find rosie_map_controller)/launch/rosie_lidar_reader.launch"/>

	<!-- launch map reader/provider -->
	<include file="$(find rosie_map_controller)/launch/rosie_map_handling.launch"/>

	<!-- launch map localization -->
	<include file="$(find rosie_map_controller)/launch/rosie_map_localization.launch"/>

	<!-- launch map updater -->
	<include file="$(find rosie_map_updater)/launch/rosie_map_updater.launch"/>

</launch>
